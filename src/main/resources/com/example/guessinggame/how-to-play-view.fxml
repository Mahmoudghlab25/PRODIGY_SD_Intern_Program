<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<AnchorPane stylesheets="@HowToPlay.css" xmlns="http://javafx.com/javafx"
            fx:id="anchorPane"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.guessinggame.HowToPlayController"
            prefHeight="563.0" prefWidth="1000.0">
    <VBox alignment="CENTER" spacing="20.0" fx:id="centeredVBox">
        <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
        </padding>
        <Label fx:id="label" style="-fx-font-weight: bold; -fx-font-size: 16;"/>
        <Button text="Return To Main Menu" onAction="#returnMainMenu" styleClass="button-main-menu"/>
    </VBox>

</AnchorPane>

